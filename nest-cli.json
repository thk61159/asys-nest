{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": false,
  "compilerOptions": {
    "builder":"tsc",
    "tsConfigPath": "tsconfig.json",
    "typeCheck": true,
    "webpack": false,
    "webpackConfigPath": "webpack.config.js",
    "deleteOutDir": true,
    "plugins": []
  },
  "generateOptions":{
    "spec": true,
    "flat": true,
    "baseDir":""
  }
}
